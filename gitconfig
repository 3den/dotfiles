[core]
  excludesfile = ~/.gitignore
[push]
  default = current
[alias]
  co = checkout
  br = branch
  aa = add --all
  up = pull --rebase origin master
[color]
  ui = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[filter "media"]
  clean = git media clean %f
  smudge = git media smudge %f
  required = true
[user]
  email = edendroid@gmail.com
  name = Marcelo Eden
[branch]
  autoSetupMerge = always
[pull]
  rebase = false
