[core]
    excludesfile = ~/.gitignore
[push]
	default = current
[alias]
	co = checkout
    br = branch
    aa = add --all
    up = pull --rebase origin master
[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
